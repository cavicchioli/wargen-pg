  <div class="hold-transition login-page" ng-controller="UserController as user">
	<div class="login-box">
	  <div class="login-logo">
		<a href="/"><b>WAR</b>GEN</a>
	  </div><!-- /.login-logo -->
	  <div class="login-box-body">
		<p class="login-box-msg">Informe sua credencial para iniciar</p>

		<form method="post" ng-submit="user.doLogin()">
		  <div class="form-group has-feedback">
			<input name="email" type="email" class="form-control" placeholder="Email" ng-model="user.loginData.email">
		  </div>
		  <div class="form-group has-feedback">
			<input name="senha" type="password" class="form-control" placeholder="Senha" ng-model="user.loginData.senha">
		  </div>
		  <div class="row">
			<div class="col-xs-8">
			</div><!-- /.col -->
			<div class="col-xs-4">
			  <button type="submit" class="btn btn-primary btn-block btn-flat">Entrar</button>
			</div><!-- /.col -->
		  </div>
		</form>

		<a href="#">Esqueci a senha</a><br>
